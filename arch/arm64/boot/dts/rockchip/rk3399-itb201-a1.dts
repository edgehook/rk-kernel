	test-power {
		status = "okay";
	};
};

/*
* CPU power supply.
*/
&cpu_l0 {
	cpu-supply = <&vdd_cpu_l>;
};
&cpu_l1 {
	cpu-supply = <&vdd_cpu_l>;
};
&cpu_l2 {
	cpu-supply = <&vdd_cpu_l>;
};
&cpu_l3 {
	cpu-supply = <&vdd_cpu_l>;
};
&cpu_b0 {
	cpu-supply = <&vdd_cpu_b>;
};
&cpu_b1 {
	cpu-supply = <&vdd_cpu_b>;
};


/************
* Ethernet
* Local MAC.
************/
&gmac {
	phy-supply = <&vcc_phy>;
	phy-mode = "rgmii";
	clock_in_out = "input";
	snps,reset-gpio = <&gpio3 16 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	snps,reset-delays-us = <500 20000 75000>;
	assigned-clocks = <&cru SCLK_RMII_SRC>;
	assigned-clock-parents = <&clkin_gmac>;
	pinctrl-names = "default";
	pinctrl-0 = <&rgmii_16ma_pins &rgmii_phy_reset>;
	enable_phy_delay = <0>;
	tx_delay = <0x2a>;
	rx_delay = <0x1e>;
	status = "okay";
};

/****************
* PCIe 
****************/
&pcie_phy {
	status = "okay";
};

&pcie0 {
	ep-gpios = <&gpio1 23 GPIO_ACTIVE_HIGH>;
	num-lanes = <2>;
	pinctrl-names = "default";
	pinctrl-0 = <&pcie_clkreqn &m2_pcie_reset_l>;
	status = "okay";
};

/******************** 
* USB 3.0 for LTE
********************/
&tcphy0 {
	status = "okay";
};

&usbdrd3_0 {
	status = "okay";
};

&usbdrd_dwc3_0 {
	status = "okay";
	dr_mode = "host";
};

&u2phy0 {
	status = "okay";

	u2phy0_otg: otg-port {
		vbus-supply = <&vcc_otg_vbus>;
		status = "okay";
	};
};

/*
* USB2.0 interface.
* 0: LAN2
* 1: USB2.0 HUB.
*/
&usb_host0_ehci {
	status = "okay";
};

&usb_host0_ohci {
	status = "okay";
};

&u2phy0 {
	status = "okay";
	u2phy0_host: host-port {
		status = "okay";
	};
};

&usb_host1_ehci {
	status = "okay";
};

&usb_host1_ohci {
	status = "okay";
};

&u2phy1 {
	status = "okay";

	u2phy1_host: host-port {
		status = "okay";
	};
};

&spi2 {
	status = "okay";
	max-freq = <48000000>;
	/*/delete-property/ dmas;
	/delete-property/ dma-names;*/

	m25p80@0 {
		compatible = "jedec,spi-nor";
		spi-max-frequency = <24000000>;
		reg = <0>;
		//spi-cpol;
		//spi-cpha;
	};

};

/********************************
**
**   Display subsystem.
**
*********************************/
&display_subsystem {
	status = "okay";
};

&vopb {
	assigned-clocks = <&cru DCLK_VOP0_DIV>;
	assigned-clock-parents = <&cru PLL_VPLL>;
	status = "okay";
};
&vopb_mmu {
	status = "okay";
};

&vopl {
	assigned-clocks = <&cru DCLK_VOP1_DIV>;
	assigned-clock-parents = <&cru PLL_CPLL>;
	status = "okay";
};
&vopl_mmu {
	status = "okay";
};

&dmac_bus {
	iram = <&iram>;
	rockchip,force-iram;
};


